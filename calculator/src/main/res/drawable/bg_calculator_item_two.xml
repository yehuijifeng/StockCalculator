<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#DDDEDE"/>
            <stroke android:color="#D5D5D6" android:width="@dimen/calculator_item_stroke"/>
            <corners android:radius="@dimen/calculator_item_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FCFCFC"/>
            <corners android:radius="@dimen/calculator_item_radius"/>
        </shape>
    </item>
</selector>