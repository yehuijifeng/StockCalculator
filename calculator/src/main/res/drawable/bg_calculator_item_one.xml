<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#DADBDE"/>
            <stroke android:color="#CDD0D4" android:width="@dimen/calculator_item_stroke"/>
            <corners android:radius="@dimen/calculator_item_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#F0F0F0"/>
            <corners android:radius="@dimen/calculator_item_radius"/>
        </shape>
    </item>
</selector>